{
  "name": "snap-talk",
  "version": "0.0.0",
  "scripts": {
    "generate:styles": "sass --update src/assets/CSS/themes/theme-light.scss:src/assets/CSS/themes/theme-light.css && sass --update src/assets/CSS/themes/theme-dark.scss:src/assets/CSS/themes/theme-dark.css && sass --update src/assets/CSS/layouts/layout-light.scss:src/assets/CSS/layouts/layout-light.css && sass --update src/assets/CSS/layouts/layout-dark.scss:src/assets/CSS/layouts/layout-dark.css",
    "start": "ng serve",
    "build": "npm run generate:styles && ng build --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@types/socket.io-client": "^3.0.0",
    "ngx-cookie-service": "^17.0.1",
    "primeflex": "^3.3.1",
    "primeng": "^17.0.0",
    "pusher-js": "^8.4.0-rc2",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.7.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.6",
    "@angular/cli": "^17.0.6",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2"
  }
}
